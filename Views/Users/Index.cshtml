@{
    ViewData["Title"] = "Home";
}

@model List<CurrentStock>

<div>
    <h1>Shop our items</h1>
    @foreach (CurrentStock item in Model) {
        <hr />
        <div>
            <p>Make @item.Make</p>
            <p>Model @item.Model</p>
            <p>Vin @item.Vin</p>
            <p>License @item.License</p>
            <p>Price R @item.Price</p>
        </div>
        <hr />
    }

</div>
